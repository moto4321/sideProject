ading: false,\n      };\n    case POST_EDIT_UPLOADING_FAILURE:\n      return {\n        ...state,\n        error: action.payload,\n        loading: false,\n      };\n    case CATEGORY_FIND_REQUEST:\n      return {\n        ...state,\n        posts: [],\n        loading: true,\n      };\n    case CATEGORY_FIND_SUCCESS:\n      return {\n        ...state,\n        categoryFindResult: action.payload,\n        loading: false,\n      };\n    case CATEGORY_FIND_FAILURE:\n      return {\n        ...state,\n        categoryFindResult: action.payload,\n        loading: false,\n      };\n    case SEARCH_REQUEST:\n      return {\n        ...state,\n        posts: [],\n        searchBy: action.payload,\n        loading: true,\n      };\n    case SEARCH_SUCCESS:\n      return {\n        ...state,\n        searchBy: action.payload,\n        searchResult: action.payload,\n        loading: false,\n      };\n    case SEARCH_FAILURE:\n      return {\n        ...state,\n        searchResult: action.payload,\n        loading: false,\n      };\n    default:\n      return state;\n  }\n}\n\n//확인","/home/moto4321/React/react-blog/client/src/redux/reducers/authReducer.js",[],"/home/moto4321/React/react-blog/client/src/redux/reducers/commentReducer.js",[],"/home/moto4321/React/react-blog/client/src/redux/sagas/index.js",[],"/home/moto4321/React/react-blog/client/src/components/auth/RegisterModal.js",[],"/home/moto4321/React/react-blog/client/src/components/auth/LoginModal.js",[],"/home/moto4321/React/react-blog/client/src/components/search/searchinput.js",[],"/home/moto4321/React/react-blog/client/src/components/post/Category.js",[],"/home/moto4321/React/react-blog/client/src/components/post/PostCardOne.js",[],"/home/moto4321/React/react-blog/client/src/components/spinner/Spinner.js",[],"/home/moto4321/React/react-blog/client/src/components/editor/EditorConfig.js",[],"/home/moto4321/React/react-blog/client/src/components/editor/UploadAdapter.js",[],"/home/moto4321/React/react-blog/client/src/